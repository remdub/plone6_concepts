<div class="p-3 pt-0">
    <div class="row">
        <div tal:repeat="item view/infos" class="col-md-4">
            <div class="card h-100">
                <div class="card-body">
                    <h3 class="card-title">${item/title}</h3>
                    <p class="card-text">${item/description}</p>
                    <a href="${item/url}" class="btn btn-primary">More info</a>
                </div>
            </div>
        </div>
    </div>
</div>
